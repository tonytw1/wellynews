<ehcache>

    <diskStore path="java.io.tmpdir/searchwellington"/>
        
   	<cache name="geocodes"
        maxElementsInMemory="500"
        eternal="false"
        timeToIdleSeconds="86400"
        timeToLiveSeconds="86400"
        overflowToDisk="false"
        />
          
    <defaultCache
        maxElementsInMemory="500"
        eternal="false"
        timeToIdleSeconds="7200"
        timeToLiveSeconds="7200"
        overflowToDisk="false"
        />
    
		
	<!-- cache external RSS SyndFeeds -->
	<cache name="feeds"
		maxElementsInMemory="300"
		eternal="false"
		timeToIdleSeconds="14400"
		timeToLiveSeconds="14400"
		overflowToDisk="false"
		/>
	
	
	<!-- cache external Calendars -->
	<cache name="calendars"
		maxElementsInMemory="100"
		eternal="false"
		timeToIdleSeconds="7200"
		timeToLiveSeconds="7200"
		overflowToDisk="false"
		/>
			
</ehcache>