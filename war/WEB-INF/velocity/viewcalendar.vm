
#set ($heading = $feed.name)

#parse("includes/header.vm")

<div class="page">
#parse("includes/topbar.vm")


<div class="left">
#parse("includes/tag_tree.vm")
</div>

<div class="main">
<h2>$heading</h2>

#if ($feed.editUrl) 
	<ul>
		<li class="admin"><a href="${site_information.url}$feed.editUrl">Edit</a></li>		
	</ul>
#end


<p>This calendar is provided by a third party.</p>


#if ($events) 
<p>This calendar contains the following events.</p>



<div class="section" id="maincontent">
<ul class="flat_list">
	#foreach($event in $events)
		#parse("includes/event.vm")
	#end
</ul>
</div>






#else
<p>There was a problem loading this calendar. No items could be displayed.</p>
#end


</div>

<div class="right">
#parse("includes/login.vm")


<div class="box">
#parse("includes/righthand_content.vm")
</div>

</div>

</div>


#parse("includes/footer.vm")