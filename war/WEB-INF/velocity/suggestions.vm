#parse("includes/header.vm")

<div class="page">
#parse("includes/topbar.vm")



<div class="left">
#parse("includes/tag_tree.vm")
</div>





<div class="main">

<h2>$heading</h2>


<p>New newsitems found on local feeds which may be suitable for inclusion.</p>


<ul>
#foreach($suggestion in $suggestions)
	#if($velocityCount <= 10) 
	<li>
		<ul>
		
			<li>
			<ul class="flat_list">
				<li><a href="$suggestion.url">$suggestion.url</a></li>
				<li class="publisher">$suggestion.feed.name</li>			
				<li class="admin"><a href="$adminUrlBuilder.getSuggestionAcceptUrl($suggestion)">accept</a></li>
				<li class="admin"><a href="$adminUrlBuilder.getSuggestionSuppressUrl($suggestion)">ignore</a></li>
			</ul>
			</li>
			
		</ul>
	</li>	
	#end
#end
</ul>


</div>




<div class="right">

#parse("includes/login.vm")

<div class="box">
#parse("includes/latest_newsitems.vm")
<br clear="all"/>

</div>


</div>

</div>

#parse("includes/footer.vm")