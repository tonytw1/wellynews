#parse("includes/header.vm")

<div class="page">
#parse("includes/topbar.vm")



<div class="left">
#parse("includes/tag_tree.vm")
</div>





<div class="main">

	<h2>$heading #if ($user.profilename) - $user.profilename #end</h2>
	
	#if ($user)
		#if ($user.url)
			<p>Blog or website: <a href="$user.url">$user.url</a></p>
		#end		
		
		
		
				
		<h3>Submissions</h3>
		
		#if ($submitted.size() > 0) 
			<ul class="flat_list">
				#foreach($item in $submitted)
					<li class="content_item">
						<ul class="main_content_item">
							#parse("includes/content_element.vm")
						</ul>
					</li>
				#end
			</ul>
		#else
			<p>This user has not made any submissions.</p>
		#end
	#end

</div>




<div class="right">

#parse("includes/login.vm")

<div class="box">
#parse("includes/latest_newsitems.vm")

<br clear="all"/>

</div>


</div>

</div>

#parse("includes/footer.vm")