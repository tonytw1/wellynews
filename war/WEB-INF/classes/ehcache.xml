<ehcache>

  
    <diskStore path="java.io.tmpdir"/>

    <defaultCache
        maxElementsInMemory="500"
        eternal="false"
        timeToIdleSeconds="7200"
        timeToLiveSeconds="7200"
        overflowToDisk="false"
        />
           		
	<cache name="nz.co.searchwellington.model.ResourceImpl.tags"
		maxElementsInMemory="256"
		timeToIdleSeconds="7200"
        timeToLiveSeconds="7200"
		overflowToDisk="false"
		/>		
			    
	<cache name="nz.co.searchwellington.model.CommentFeed"
		maxElementsInMemory="200"
		timeToIdleSeconds="7200"
        timeToLiveSeconds="7200"
		overflowToDisk="false"
		/>
				
	<cache name="nz.co.searchwellington.model.CommentFeed.comments"
		maxElementsInMemory="200"
		timeToIdleSeconds="7200"
        timeToLiveSeconds="7200"
		overflowToDisk="false"
		/>
		
	<cache name="nz.co.searchwellington.model.Comment"
		maxElementsInMemory="500"
		eternal="false"
		timeToIdleSeconds="7200"
		timeToLiveSeconds="7200"
		overflowToDisk="false"
		/>	
		
		
		
	<cache name="nz.co.searchwellington.model.DiscoveredFeed"
		maxElementsInMemory="500"
		eternal="false"
		timeToIdleSeconds="7200"
		timeToLiveSeconds="7200"
		overflowToDisk="false"
		/>		
		
			
	<cache name="nz.co.searchwellington.model.DiscoveredFeed.references"
		maxElementsInMemory="500"
		eternal="true"
		overflowToDisk="false"
		/>
		
	
	<cache name="nz.co.searchwellington.config.Geocode"
		maxElementsInMemory="500"
		eternal="false"
		timeToIdleSeconds="7200"
		timeToLiveSeconds="7200"
		overflowToDisk="false"
		/>
		
	<cache name="nz.co.searchwellington.model.Suppression"
		maxElementsInMemory="500"
		eternal="false"
		timeToIdleSeconds="7200"
		timeToLiveSeconds="7200"
		overflowToDisk="false"
		/>
				
	<cache name="nz.co.searchwellington.model.ResourceImpl"
		maxElementsInMemory="500"
		eternal="true"
		overflowToDisk="false"
		/>
	
	<cache name="nz.co.searchwellington.config.Config"
		maxElementsInMemory="1"
		eternal="true"
		overflowToDisk="false"
		/>


	<cache name="nz.co.searchwellington.model.Tag"
		maxElementsInMemory="1000"
		eternal="true"
		overflowToDisk="false"
		/>
		
		
		
	<cache name="nz.co.searchwellington.model.Tag.children"
		maxElementsInMemory="1000"
		timeToIdleSeconds="7200"
        timeToLiveSeconds="7200"	
		overflowToDisk="false"
		/>
		
					
	<cache name="nz.co.searchwellington.model.WebsiteImpl.calendars"
		maxElementsInMemory="500"
		timeToIdleSeconds="7200"
        timeToLiveSeconds="7200"
		overflowToDisk="false"
		/>

	<cache name="nz.co.searchwellington.model.WebsiteImpl.newsitems"
		maxElementsInMemory="500"
		timeToIdleSeconds="7200"
        timeToLiveSeconds="7200"
		overflowToDisk="false"
		/>

	<cache name="nz.co.searchwellington.model.WebsiteImpl.feeds"
		maxElementsInMemory="500"
		timeToIdleSeconds="7200"
        timeToLiveSeconds="7200"
		overflowToDisk="false"
		/>
		
	<cache name="nz.co.searchwellington.model.WebsiteImpl.discoveredFeeds"
		maxElementsInMemory="500"
		timeToIdleSeconds="7200"
        timeToLiveSeconds="7200"
		overflowToDisk="false"
		/>	
		
	<cache name="nz.co.searchwellington.model.WebsiteImpl.watchlist"
		maxElementsInMemory="500"
		timeToIdleSeconds="7200"
        timeToLiveSeconds="7200"
		overflowToDisk="false"
		/>	
				
</ehcache>