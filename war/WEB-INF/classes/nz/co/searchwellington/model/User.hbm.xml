<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class 	name="nz.co.searchwellington.model.UserImpl" 
			proxy="nz.co.searchwellington.model.User"
			table="user" >
			
 
 	
 	
        <id name="id" type="integer" unsaved-value="0" >
            <column name="id" sql-type="integer" not-null="true"/>
                    <generator class="native"/>
        </id>

        <property name="username">
            <column name="username" length="255" not-null="true"/>
        </property>
        
         <property name="password">
            <column name="password" length="255" not-null="true"/>
        </property>
        
        
      
               
    </class>

</hibernate-mapping>
